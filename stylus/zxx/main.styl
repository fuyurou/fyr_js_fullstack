bg_color=#12ef56
box_size=100px

vendor(prop,args)
    -webkit-{prop} args 
    -moz-{prop} args
    -ms-{prop} args
    {prop} args

border-radius()
    vendor('border-radius',arguments)

box-shadow()
    vendor('box-shadow',arguments)

// 能不能我只写标准属性，前缀帮生成？mixin 混合
// boder-radius 这些兼容性都得写
.box
    background-color bg_color
    width box_size
    height  box_size
    boder-radius (5px)
    box-shadow (1 1 10px rgba(0,0,0,.5))